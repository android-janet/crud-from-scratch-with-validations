<div class="page-header">
    <h1>Update Status</h1>
</div>

<%= render partial: 'shared/errors', locals: {object: @status} %>

<%= form_for @status do |f| %>

<p>
  <%= f.label :user %> <br>
  <%= f.text_field :user %>
</p>
<p>
  <%= f.label :status %> <br>
  <%= f.text_field :status %>
</p>

<p>
  <%= f.submit %><br>
  <%= link_to 'Cancel', statuses_path %>
</p>
<% end %>
