<div class="page-header">
  <h1>What are you up to? <br> <small>Create a new status.</small></h1>
</div>

<%= render partial: 'shared/errors', locals: {object: @status} %>

<%= form_for @status do |f| %>
<p>
  <%= f.label :user %> <br>
  <%= f.text_field :user %>
</p>
<p>
  <%= f.label :status %> <br>
  <%= f.text_field :status %>
</p>

<p>
  <%= f.submit %>
</p>

<% end %>
